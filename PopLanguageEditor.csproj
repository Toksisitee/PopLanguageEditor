<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icon-exit.ico" />
    <None Remove="icon-new.ico" />
    <None Remove="icon-open.ico" />
    <None Remove="icon-save.ico" />
    <None Remove="icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icon-exit.ico" />
    <Resource Include="icon-new.ico" />
    <Resource Include="icon-open.ico" />
    <Resource Include="icon-save.ico" />
    <Resource Include="icon.ico">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

</Project>
